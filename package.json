{
  "name": "@inkcop/exchange-client",
  "version": "0.0.2",
  "description": "Framework-agnostic client SDK for InkCop Exchange Server",
  "author": "JERR <jerr@foxmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.zh-CN.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.8",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "inkcop",
    "exchange",
    "sse",
    "client",
    "sdk",
    "ai",
    "llm",
    "realtime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jooler/inkcop-exchange-client.git"
  },
  "bugs": {
    "url": "https://github.com/jooler/inkcop-exchange-client/issues"
  },
  "homepage": "https://github.com/jooler/inkcop-exchange-client#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}

